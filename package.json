{
  "name": "symfony-translation-shortcut",
  "displayName": "Symfony Translation Shortcut",
  "description": "Quick keyboard shortcut to add translation keys to Symfony YAML files and auto-replace with Twig functions",
  "version": "0.2.2",
  "publisher": "Soma",
  "author": "Mariollet",
  "license": "MIT",
  "keywords": [
    "symfony",
    "i18n",
    "internationalization",
    "translation",
    "yaml",
    "twig"
  ],
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "icon": "icon.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mariollet/vs_auto_localize.git"
  },
  "bugs": {
    "url": "https://github.com/Mariollet/vs_auto_localize/issues"
  },
  "homepage": "https://github.com/Mariollet/vs_auto_localize#readme",
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "symfony-translation-shortcut.addTranslationKey",
        "title": "Add Symfony Translation Key"
      }
    ],
    "keybindings": [
      {
        "command": "symfony-translation-shortcut.addTranslationKey",
        "key": "ctrl+alt+i",
        "when": "editorTextFocus"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map"
  },
  "devDependencies": {
    "@types/node": "22.x",
    "@types/vscode": "^1.103.0",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.2",
    "webpack": "^5.101.3",
    "webpack-cli": "^6.0.1"
  }
}
